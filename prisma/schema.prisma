generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                  Int      @id @default(autoincrement())
  email               String   @unique @db.VarChar(255)
  password            String   @db.VarChar(255)
  nombre              String   @db.VarChar(255)
  codigo_habilitacion String   @db.VarChar(20)
  role                Role     @default(USER)
  created_at          DateTime @default(now())
  updated_at          DateTime @updatedAt

  @@map("users")
}

model ControlLote {
  id                  Int       @id
  numero_lote         Int?
  fecha_creacion      DateTime? @db.Date
  nombre_ips          String?   @db.VarChar(200)
  codigo_habilitacion String?   @map("codigo_habilitación") @db.VarChar(12)
  cantidad_facturas   Decimal?  @db.Decimal(18, 0)
  valor_reclamado     Decimal?  @db.Decimal(18, 0)
  nombre_envio        String    @db.VarChar(50)
  tipo_envio          String?   @db.VarChar(50)

  @@map("control_lotes")
}

model Inconsistencia {
  inconsistencia_id                             Int       @id
  Numero_factura                                String?   @db.Text
