  tipo_validacion                               String?   @db.VarChar(100)
  observacion                                   String?   @db.Text
  tipo_servicio                                 String?   @db.VarChar(100)
  codigo_del_servicio                           String?   @db.Text
  descripcion_servicio                          String?   @db.Text
  cantidad                                      Int?
  valor_unitario                                Decimal?  @db.Decimal(18, 2)
  valor_total                                   Decimal?  @db.Decimal(18, 2)
  fecha                                         DateTime? @db.Date
  lote_de_carga                                 String?   @db.VarChar(50)
  id_factura_furips1                            String?   @db.Text
